doctype html
block vars
  - var bodyclass = null;
html(lang='en')
  head
    block head
      meta(charset='utf-8')
      | !{ livereloadScript }
      meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
      meta(name='viewport', content='width=device-width')
      title
        block title
          = locals.name
      link(rel='alternate', href=locals.url+'/feed.xml', type='application/rss+xml', title=locals.description)
      link(rel='stylesheet', href='http://fonts.googleapis.com/css?family=Lora:400,700,400italic,700italic|Anonymous+Pro:400,700,400italic,700italic|Merriweather:400,700,300')
      link(rel='stylesheet', href='css/main.css')
      link(rel='stylesheet', href='css/responsiveslides.css')
      link(href="https://fonts.googleapis.com/css?family=Roboto" rel="stylesheet")
      script(src="https://code.jquery.com/jquery-3.2.1.js", integrity="sha256-DZAnKJ/6XZ9si04Hgrsxu/8s717jcIzLy3oi35EouyE=", crossorigin="anonymous", 
      type="text/javascript")
      script(src="js/responsiveslides.min.js", type="text/javascript")
      script(src="js/slideshow_instance.js", type="text/javascript")
  body(class=bodyclass)
    header.header
      div.content-wrap
        block header
          h1.tagline 
            | Lake Worth FL Real Estate Attorney
            br 
            a.header_phone(href="tel:+1-561-251-4993") 561-251-4993
          div.logo
            a(href='/posada/index.html')
              img(src='cdp-icon.jpg')
            a(href='/posada/index.html')
              div
                span.span-break.bottom-border= locals.name
                span.span-break.caps= locals.job_title
            // p.description= locals.description
            // a.main_phone(href="tel:+1-561-251-4993") CALL NOW
          include nav
    div#content
      div.content-wrap
        block content
          h2 You have reached this page by mistake. Please hit the back button. {templates/layout.jade}
    footer
      div.content-wrap
        block footer
          section.about
            div.logo
              a(href='/index.html')
                img(src='cdp-icon.jpg')
              a(href='/index.html')
                div
                  span.span-break.bottom-border= locals.name
                  span.span-break.caps= locals.job_title
            !=contents['phone.md'].html
          section.copy
            p &copy; #{ new Date().getFullYear() } #{ locals.owner }, All Rights Reserved. | The hiring of a lawyer is an important decision that should not be based solely upon advertisement.
            p 
              ul 
                li lake worth fl real estate law,
                li lake worth fl real estate lawyer,
                li lake worth fl real estate attorney,
                li lake worth fl real estate law firm,
                li lake worth fl estate planning lawyer, 
                li lake worth fl estate planning attorney, 
                li lake worth fl probate lawyer, 
                li lake worth fl probate attorney, 
                li lake worth fl probate law firm, 
                li lake worth fl probate law firms, 
                li lake worth fl landlord tenant disputes,
                li lake worth fl landlord tenant laws, 
                li lake worth fl consumer law attorney, 
                li lake worth fl consumer law practice
    
    script(src="js/nav_hider.js", type="text/javascript")
